{
      "name": "Hacker World World Chain Bot",
      "description": "MOST POWERFUL BOT",
      "logo": "https://telegra.ph/file/fa506237bdb358a8771af.jpg",
      "keywords": [
        "telegram",
        "telethon",
        "telegram-bot",
        "HackerWorld",
        "SpamBots"
      ],   
  "repository": "https://github.com/hackerworldyt",
  "website": "https://github.com/hackerworldyt",
  "success_url": "https://t.me/iamhackerworld",
  "env": {
             "TOKEN": {
        "description": "A Telegram bot token.",
        "value": "",
        "required": true
      },
      "ON9BOT_TOKEN": {
        "description": "Another Telegram bot token for the virtual player bot. Can be the same as",
        "value": "",
        "required": true
      },
      "DB_URI": {
        "description": "A PostgresSQL database URI.",
        "value": "",
        "required": false
      },
      "PROVIDER_TOKEN": {
        "description": "A Telegram payment provider token.",
        "value": "",
        "required": false
      },
      "OWNER_ID": {
        "description": "Telegram user id of the bot owner.",
        "value": "5117196135",
        "required": true
      },
      "ADMIN_GROUP_ID": {
        "description": "Telegram group id of the bot admin group. Error messages and word addition requests are sent here.",
        "value": "",
        "required": true
      },
      "OFFICIAL_GROUP_ID": {
        "description": "Telegram group id of the official group of the bot.",
        "value": "https://t.me/HwBotSupport",
        "required": true
      },
      "WORD_ADDITION_CHANNEL_ID": {
        "description": "Telegram channel id of the channel to announce word additions.",
        "value": "",
        "required": false
      },
      "VIP": {
        "description": "A list of Telegram user ids designated as VIPs.",
        "value": "",
        "required": false
      },
      "VIP_GROUP": {
        "description": "A list of Telegram group ids designated as VIP groups.",
        "value": "",
        "required": false
      },
        "HEROKU_API_KEY": {
            "description": "Your Heroku api key to update your bot",
            "value": "",
            "required": false 
        },
        "HEROKU_APP_NAME": {
            "description": "Your app name to Update your bot",
          "value": "",
          "required": false  
        }
    },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "12"
      }
    }
  ],
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
